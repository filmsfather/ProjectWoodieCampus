{
  "name": "woodie-campus",
  "version": "1.0.0",
  "description": "Woodie Campus - 에빙하우스 망각곡선 기반 학습 플랫폼",
  "private": true,
  "scripts": {
    "dev": "docker-compose up -d",
    "dev:build": "docker-compose up --build -d",
    "dev:logs": "docker-compose logs -f",
    "dev:stop": "docker-compose stop",
    "dev:down": "docker-compose down",
    "prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "prod:build": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build -d",
    "prod:stop": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml stop",
    "prod:down": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml down",
    "clean": "docker-compose down -v --rmi all --remove-orphans",
    "setup": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "education",
    "learning",
    "ebbinghaus",
    "spaced-repetition"
  ],
  "author": "Woodie Campus Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}